
<%- include("./partials/header") -%>
    <h1>BLOGS PAGE</h1>

<% blogs.forEach((blog)=>{ %>
    <div>
        <h2> <%= blog.title %></h2>
        <img class="" src="<%= blog.image %>" alt="">
        <br>
        <span> <%= blog.created %></span>
        <p><%= blog.body.substring(0, 100) %>...</p>
        <a href="/blogs/<%= blog._id %>">Read more.</a>
    </div>
    
<%}) %>

<%- include("./partials/footer") -%>